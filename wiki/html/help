<script language="javascript">
  function toggleTOC() {
    var toc = document.getElementById('TOC');
    if (document.defaultView.getComputedStyle(toc, null).display == 'none') {
      toc.style.display = 'inline';
    } else {
      toc.style.display = 'none';
    }
  }
</script>
</head>
